{
  "name": "hiring-test-react",
  "private": true,
  "scripts": {
    "lint-fix": "tslint --project tsconfig.json --fix; npm run prettier -- --write",
    "lint": "tsc --project tsconfig.json --noEmit && tslint --project tsconfig.json && npm run prettier -- --list-different",
    "prettier": "prettier '*.{ts,tsx}' 'src/**/*.{ts,tsx}' '.storybook/**/*.{js,ts,tsx}'",
    "start": "start-storybook",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "blueimp-md5": "^2.12.0",
    "dayjs": "^1.8.17"
  },
  "peerDependencies": {
    "emotion": ">=10.0.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/blueimp-md5": "^2.7.0",
    "@types/jest": "^24.0.23",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "emotion": "^10.0.23",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/*.test.ts?(x)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ]
  }
}
